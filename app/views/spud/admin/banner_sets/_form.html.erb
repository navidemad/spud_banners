<%= form_for @banner_set, :url => path, :html => {:class => 'form-horizontal spud_banner_set_form'} do |f| %>

  <%= error_messages_for(f.object) %>

  <fieldset>
    <div class="form-group">
      <%= f.label :name, "Name",:class => "control-label col-sm-4" %>
      <div class="col-sm-8">
        <%= f.text_field :name, :class => "form-control" %>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :width, "Width", :class => "control-label col-sm-4" %>
      <div class="col-sm-8">
        <%= f.text_field :width, :class => "form-control" %>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :height, "Height", :class => "control-label col-sm-4" %>
      <div class="col-sm-8">
        <%= f.text_field :height, :class => "form-control" %>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :cropped, "Cropped", :class => "control-label col-sm-4" %>
      <div class="col-sm-8">
        <label class="checkbox">
          <%= f.check_box :cropped %>  
        </label>
        
      </div>
    </div>
  </fieldset>

<% end %>